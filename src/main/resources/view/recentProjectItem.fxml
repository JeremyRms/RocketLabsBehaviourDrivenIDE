<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.102-ea"
            onMouseClicked="#openProject">
    <VBox fx:id="vBox">
        <Label fx:id="projectName"/>
        <Label fx:id="projectPath"/>
    </VBox>
    <Button fx:id="remove" prefWidth="20.0" AnchorPane.rightAnchor="0"/>
</AnchorPane>
